        "baseUrl": "./",
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "module": "AMD",
        "moduleResolution": "node",
        "paths": {
            "*": [
                "sources/edo/core/client/*",
                "sources/edo/edo-client/client/*",
                "sources/eo/core/client/*",
                "sources/eo/custom-ui/client/*",
                "sources/eo/registry/client/*",
